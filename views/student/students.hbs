<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default whiteBg">
                <div class="panel-heading">
                    <h3 class="panel-title"><a href="/dashboard/">Home</a></h3>
                </div>
                <div class="panel-body">
                    <table id="customTable" class="table table-striped table-bordered nowrap table-hover" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Class Level</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each this.students as |student|}}
                            <tr>
                                <td>{{student.person.first_name}} {{student.person.last_name}}</td>
                                <td>{{student.class_level}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
