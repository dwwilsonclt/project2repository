<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><a href="#">Departments</a> > <a href="#">{{dept}}</a> > <a href="#">Professors</a> </h3>
                </div>
                <div class="panel-body">
                    <table id="customTable" class="table table-striped table-bordered nowrap table-hover" cellspacing="0" width="100%">
                        <thead>
                    <th>Room #</th>
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Email Address</th>
                    <th>Phone</th>
                    <th>Extention</th>
                    <th>Address</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Zip Code</th>
                </thead>
                <tbody>
                    {{#professors}}
                    <tr>
                        <td>{{room_id}}</td>
                        <td>{{person.last_name}}</td>
                        <td>{{person.first_name}}</td>
                        <td><a href="mailto:{{email}}" target="_top">{{email}}</a></td>
                        <td>{{phone}}</td>
                        <td>{{phone_extension}}</td>
                        <td>{{person.address}}</td>
                        <td>{{person.city}}</td>
                        <td>{{person.state}}</td>
                        <td>{{person.zip_code}}</td>
                    </tr>
                    {{/professors}}
                </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
